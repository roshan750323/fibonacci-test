pipeline {
  agent any
  stages {
    stage('Build Docker image') {
      steps {
        script {
          docker.build('roshan750/fibonacci:4.0')
        }
      }
    }
    stage('Push to Docker Hub') {
      steps {
        script {
          docker.withRegistry('https://registry.hub.docker.com', 'docker_roshan') {
            docker.image('roshan750/fibonacci:4.0').push()
          }
        }
      }
    }
    stage('Cleanup') {
      steps {
        cleanWs()
      }
    }
  }
}

