pipeline {
  agent any
  stages {
    stage('Build Docker image') {
      steps {
        script {
          def builtImage = docker.build("fibonnaci:3.0")
          env.BUILT_IMAGE = builtImage
        }
      }
    }
    stage('Push to Docker Hub') {
      steps {
        script {
          docker.withRegistry('https://registry.hub.docker.com', 'docker_roshan') {
            env.BUILT_IMAGE.push()
          }
        }
      }
    }
  }
}

